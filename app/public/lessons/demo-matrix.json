{
  "id": "demo-matrix",
  "title": "Demo：2x2 矩阵变换",
  "durationMs": 9000,
  "events": [
    { "atMs": 0, "action": { "kind": "reset" } },
    { "atMs": 0, "action": { "kind": "patch", "patch": { "mode": "matrix", "matrix2x2": [[1, 0], [0, 1]] } } },
    { "atMs": 400, "action": { "kind": "tween", "to": { "matrix2x2": [[1.2, 0], [0, 1]] }, "durationMs": 900 } },
    { "atMs": 1600, "action": { "kind": "tween", "to": { "matrix2x2": [[1, 0.8], [0, 1]] }, "durationMs": 900 } },
    { "atMs": 2800, "action": { "kind": "tween", "to": { "matrix2x2": [[0.7, -0.7], [0.7, 0.7]] }, "durationMs": 1100 } },
    { "atMs": 4300, "action": { "kind": "tween", "to": { "matrix2x2": [[1, 0], [0.6, 1]] }, "durationMs": 900 } },
    { "atMs": 5600, "action": { "kind": "tween", "to": { "matrix2x2": [[0.4, 0], [0, 1.6]] }, "durationMs": 900 } },
    { "atMs": 7000, "action": { "kind": "tween", "to": { "matrix2x2": [[1, 0], [0, 1]] }, "durationMs": 1000 } },
    { "atMs": 8400, "action": { "kind": "tween", "to": { "mode": "cube" }, "durationMs": 400 } }
  ]
}

