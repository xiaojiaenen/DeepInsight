{
  "id": "demo-visual",
  "title": "Demo：立方体联动",
  "durationMs": 6000,
  "events": [
    { "atMs": 0, "action": { "kind": "reset" } },
    { "atMs": 0, "action": { "kind": "patch", "patch": { "cubeColor": "#3b82f6" } } },
    { "atMs": 300, "action": { "kind": "tween", "to": { "cubeRotation": [0, 3.14, 0] }, "durationMs": 1200 } },
    { "atMs": 1800, "action": { "kind": "tween", "to": { "cubePosition": [1.2, 0.5, 0] }, "durationMs": 800 } },
    { "atMs": 2700, "action": { "kind": "tween", "to": { "cubePosition": [0, 0.5, 1.2] }, "durationMs": 800 } },
    { "atMs": 3600, "action": { "kind": "tween", "to": { "cubeScale": [1.4, 1.4, 1.4], "cubeColor": "#ef4444" }, "durationMs": 700 } },
    { "atMs": 4700, "action": { "kind": "tween", "to": { "cubeScale": [1, 1, 1], "cubeColor": "#22c55e" }, "durationMs": 700 } }
  ]
}

